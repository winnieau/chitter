
<div class="peep_page">
  <div class="peep_list">

    <h1>Peeps</h1>
    <ul id="peeps">
      <% @peeps.reverse.each do |peep| %>
        <div id="each_peep">
          <%= peep.content %> <div>posted on: </div>
          <%= peep.created_at %>
          </div>
      <% end %>
    </ul>
  </div>
</div>
